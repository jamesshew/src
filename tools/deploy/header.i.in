%define %cstring_output_buf_and_size_returning_charptr(BUFIDX, ...)
%typemap(argout) (__VA_ARGS__) {
    // cstring_output_buf_and_size_returning_charptr's argout
    Py_XDECREF(resultobj);
    if (result != NULL)
    {
        resultobj = PyString_FromString($BUFIDX);
    }
    else
    {
        Py_INCREF(Py_None);
        resultobj = Py_None;
    }
    qfree($BUFIDX);
}
%enddef

